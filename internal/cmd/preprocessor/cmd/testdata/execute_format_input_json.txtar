# Ensure that preprocessor correctly reformats input files that are known to
# it.

unquote content/page/en.md
unquote golden/content/page/en.md

# Run the preprocessor
exec preprocessor execute --debug=all

# Verify that the source file is intact
cmp content/page/en.md golden/content/page/en.md

# Verify that the target files matche the source contents
cmp hugo/content/en/page/index.md golden/hugo/content/en/page/index.md

-- hugo/.keep --
-- content/page/site.cue --
package site
-- content/page/en.md --
>---
>title: title
>---
>{{{with sidebyside "en" "json-superset"}}}
>-- in.json --
>{"x": 5}
>-- out.json --
>{
>    "x": 5
>}
>{{{end}}}
-- golden/content/page/en.md --
>---
>title: title
>---
>{{{with sidebyside "en" "json-superset"}}}
>-- in.json --
>{
>    "x": 5
>}
>-- out.json --
>{
>    "x": 5
>}
>{{{end}}}
-- golden/hugo/content/en/page/index.md --
---
title: title
---
{{< code-tabs >}}
