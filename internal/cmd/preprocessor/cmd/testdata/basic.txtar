# Ensure that preprocessor works in the basic case of
# copying input to output
exec preprocessor execute --debug

cmp hugo/content/en/index.md hugo/content/en/index.md.golden

# Ensure that the contents of the filesystem are as expected
exec find
cmp stdout stdout.golden

-- content/en.md --
en
-- content/de.md --
de
-- hugo/content/en/index.md.golden --
en
-- hugo/content/de/index.md.golden --
de
-- stdout.golden --
content/de.md
content/en.md
hugo/content/de/index.md
hugo/content/en/index.md
